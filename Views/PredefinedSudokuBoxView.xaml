<catel:UserControl x:Class="Sudoku.Views.PredefinedSudokuBoxView"
                   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                   xmlns:catel="http://schemas.catelproject.com">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <Label Content="{Binding Number}" FontSize="48" BorderBrush="Black" BorderThickness="3" />
    </Grid>
    
</catel:UserControl>
